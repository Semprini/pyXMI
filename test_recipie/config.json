{
    "source": "./test_recipie/test.xmi",
    "root_package":"Sample",
    "model_package":"Model",
    "test_package":"Test",
    "templates": [
        { 
            "source":"entities.jdl",
            "level":"package",
            "dest":"./build/test_output{{package.path}}{{package.name}}.jdl"
        },
        { 
            "source":"pojos.java",
            "level":"class",
            "dest":"./build/test_output{{cls.package.path}}{{cls.name}}.java"
        }
    ],
    "test_templates": [
        { 
            "format":"json",
            "dest":"./build/test_output{{ins.package.path}}/{{ins.stereotype}}.json"
        }
    ],
    "types": {
        "string": "String",
        "decimal": "Double",
        "date": "Date",
        "dateTime": "DateTime",
        "long": "int",
        "boolean": "boolean",
        "integer": "int",
        "int": "int",
        "enum": "String"
    }
}